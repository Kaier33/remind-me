{
  "name": "remind-me",
  "displayName": "吃饭饭",
  "description": "Remind you to do something, eat or get off work, etc.",
  "version": "0.0.1",
  "publisher": "shehuiwang",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*" 
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayCityAqi",
        "title": "rdme"
      }
    ],
    "keybindings": [
      {
        "command": "extension.sayCityAqi",
        "key": "ctrl+shift+,",
        "mac": "ctrl+shift+,"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "axios": "^0.18.0",
    "eslint": "^4.11.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  }
}
